= semantic_form_for [:admin, @main_event, @event_prize] do |f|
  = f.semantic_errors *f.object.errors.keys
  = f.inputs do
    = f.input :level
    = f.input :name
    = f.input :is_mobile
    ~ f.input :prizes_table
  = f.submit data: { disable_with: '请稍等...' }

:javascript
    $(function() {
        cgEditor.call($("#event_prize_prizes_table"), $('form.event_prize'));
    });